{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "error",
    "spaced-comment": "error",
    "semi": "error",
    "quotes": ["error", "single"],
    "no-duplicate-imports": "error",
    "eol-last": "error",
    "no-trailing-spaces": "error",
    "max-len": ["error", 100]
  }
}
